{
  "name": "wazo-doc-api",
  "private": true,
  "description": "Wazo project documentation for developers",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/gatsby build --prefix-paths",
    "develop": "./node_modules/.bin/gatsby develop",
    "format": "prettier --write 'src/**/*.{js,jsx}' 'tests/src/*.js' 'gatsby-node.js'",
    "format:uc-doc": "prettier --print-width 100 --prose-wrap always --write 'content/uc-doc/**/*.md'",
    "check-format:uc-doc": "prettier --check --print-width 100 --prose-wrap always 'content/uc-doc/**/*.md'",
    "start": "npm run develop",
    "serve": "./node_modules/.bin/gatsby serve"
  },
  "dependencies": {
    "algoliasearch": "^3.33.0",
    "babel-plugin-styled-components": "^1.10.0",
    "btoa": "^1.2.1",
    "gatsby": "^2.32.13",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-emojis": "^0.4.3",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-remark": "^2.16.1",
    "gatsby-transformer-yaml": "^2.11.0",
    "graphiql": "^1.4.7",
    "js-yaml": "^3.13.1",
    "mobx": "^4.2.0",
    "react": "^16.8.6",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-in-viewport": "0.0.37",
    "react-instantsearch-dom": "^5.6.0",
    "react-markdown": "^4.0.8",
    "redoc": "^2.0.0-rc.8-1",
    "rss": "^1.2.2",
    "showdown": "^1.9.0",
    "stickyfill": "^1.1.1-strict",
    "striptags": "^3.1.1",
    "styled-components": "^4.2.0",
    "swagger-ui-react": "^3.52.5"
  },
  "devDependencies": {
    "dart-sass": "^1.25.0",
    "gatsby-plugin-sass": "^2.1.0",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wazo-platform/wazo-doc-api"
  }
}
